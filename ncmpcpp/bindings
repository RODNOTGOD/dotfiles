def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
def_key "ctrl-d"
    page_down

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "g"
	move_home
def_key "G"
	move_end

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "M"
	reverse_selection
def_key "t"
	reverse_selection

def_key "ctrl-t"
	remove_selection

def_key "D"
	delete_playlist_items

def_key "m"
	select_item
    scroll_down

def_key "J"
	move_selected_items_down
def_key "K"
	move_selected_items_up
